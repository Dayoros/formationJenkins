pipeline {
    agent any

    stages {
        stage('Init dossier') {
            steps {
                bat 'mkdir dossier-toto'
            }
        }
        stage('Init fichier') {
            steps {
                bat 'echo \'toto\' >> toto.txt'
            }
        }
        stage('DÃ©placement dans dossier') {
            steps {
                bat 'move toto.txt dossier-toto'
            }
        }
    }
}
